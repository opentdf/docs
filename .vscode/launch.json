{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug gen-api-docs-all",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "gen-api-docs-all"],
      "cwd": "${workspaceFolder}",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "console": "integratedTerminal"
    }
  ]
}